@model IEnumerable<FE.Models.VinculoProducto>

@{
    ViewData["Title"] = "Tipos";
}

<h1>Tipos Productos</h1>

<p>
    <a asp-action="Create">Agregar Nuevo Tipo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreVinculoProducto)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreVinculoProducto)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdVinculoProducto">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.IdVinculoProducto">Detalle</a> |
                <a asp-action="Delete" asp-route-id="@item.IdVinculoProducto">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
